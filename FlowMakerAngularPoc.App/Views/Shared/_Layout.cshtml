@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    @* The ngCloak directive is used to prevent the AngularJS html template from being briefly
        displayed by the browser in its raw (uncompiled) form while your application is loading.*@
    <style type="text/css">
        [ng\:cloak], [ng-cloak], [data-ng-cloak], [x-ng-cloak], .ng-cloak, .x-ng-cloak {
            display: none !important;
        }
    </style>
    <title>FlowMakerAngularPoc</title>
    @*Latest compiled and minified CSS*@
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css" integrity="sha384-BVYiiSIFeK1dGmJRAkycuHAHRg32OmUcww7on3RYdg4Va+PmSTsz/K68vbdEjh4u" crossorigin="anonymous">
    @*@Scripts.Render("~/bundles/libs")*@
    @Scripts.Render("~/bundles/angularjs")
    @Scripts.Render("~/bundles/angular-ui")
    @Scripts.Render("~/bundles/app")
    @Styles.Render("~/Content/css")

</head>
<body class="ng-cloack" ng-app="angularBox" ng-controller="ActionController" database="dbExample">
    <span ng-init="uiVars={}"></span>
    <span ng-init="$root.exchange={uiVars:uiVars}"></span>
    <lock-screen></lock-screen>
    <alert-message></alert-message>
    @RenderBody()
</body>
</html>