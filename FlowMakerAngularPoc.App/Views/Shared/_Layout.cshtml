@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    @* The ngCloak directive is used to prevent the AngularJS html template from being briefly
        displayed by the browser in its raw (uncompiled) form while your application is loading.*@
    <style type="text/css">
        [ng\:cloak], [ng-cloak], [data-ng-cloak], [x-ng-cloak], .ng-cloak, .x-ng-cloak {
            display: none !important;
        }
    </style>
    <title>FlowMakerAngularPoc</title>
    @*Latest compiled and minified CSS*@
    <link type="text/css" rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css" integrity="sha384-BVYiiSIFeK1dGmJRAkycuHAHRg32OmUcww7on3RYdg4Va+PmSTsz/K68vbdEjh4u" crossorigin="anonymous">
    <link type="text/css" href="~/Content/styles.less" rel="stylesheet/less" />
    <link type="text/css" href="~/Content/contextmenu.css" rel="stylesheet" />

    @Scripts.Render("~/bundles/draw2d/lib")
    

    <SCRIPT src="~/Scripts/draw2d/src/customShapes/Info.js"> </SCRIPT>
    <SCRIPT src="~/Scripts/draw2d/src/customShapes/HtmlFigure.js"> </SCRIPT>
    <SCRIPT src="~/Scripts/draw2d/src/customShapes/Condition.js"> </SCRIPT>
    <SCRIPT src="~/Scripts/draw2d/src/customShapes/Control.js"> </SCRIPT>



    <script src="~/Scripts/less/less-1.7.5.min.js"></script>
   

    

</head>

@RenderBody()

@Scripts.Render("~/bundles/angularjs")
<!-- <script src="~/Scripts/jquery/jquery.browser.js"></script>
<script src="~/Scripts/jquery/jquery-ui-1.8.23.custom.min.js"></script> -->
@Scripts.Render("~/bundles/angular-ui")
@Scripts.Render("~/bundles/app")
</html>